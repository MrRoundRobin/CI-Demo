    <PackageTags>CI; GutHub</PackageTags>
    <PackageId>$(AssemblyName)</PackageId>
    <PackageProjectUrl>https://github.com/MrRoundRobin/CI-Demo</PackageProjectUrl>
    <Version>0.0.0-alpah.1</Version>
  </PropertyGroup>

</Project>
